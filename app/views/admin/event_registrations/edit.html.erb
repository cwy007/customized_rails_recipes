<h1><%= @event.name %> / Edit Registration</h1>

<%= form_for [:admin, @event, @registration] do |f| %>

  <%= render :partial => "form", :locals => { :f => f } %>

  <div class="form-group">
    <%= f.submit "Update", :class => "btn btn-primary" %>
  </div>

<% end %>

<script type="text/javascript">
  $("form").validator();
</script>
